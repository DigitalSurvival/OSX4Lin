			</item>
			<item type='text'>
				<position value='center' />
				<text format='has gone away' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='back' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
