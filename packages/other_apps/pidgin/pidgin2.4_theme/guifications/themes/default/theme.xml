<?xml version='1.0' encoding='UTF-8' ?>
<guifications>
	<theme api='1'>
		<info>
			<name>[Default]</name>
			<version>1.0</version>
			<summary>Guifications Default Theme</summary>
			<description>The default guifications theme</description>
			<author>Gary Kramlich &lt;amc_grim@users.sf.net&gt;</author>
			<website>http://guifications.sourceforge.net</website>
		</info>
		<options>
			<ellipsis>...</ellipsis>
		</options>
		<notification type='sign-on' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='has signed on' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='sign-off' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
