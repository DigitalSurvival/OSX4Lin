			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-13%' />
				<text format='"%X" completed.' clipping='ellipsis-middle' width='0' />
			</item>
		</notification>
		<notification type='new-email' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
