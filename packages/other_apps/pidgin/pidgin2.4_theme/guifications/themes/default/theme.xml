			</item>
			<item type='text'>
				<position value='center' />
				<text format='is no longer idle' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='im-message' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
