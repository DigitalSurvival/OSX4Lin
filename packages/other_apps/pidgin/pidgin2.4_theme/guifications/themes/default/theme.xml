		<notification type='chat-invite' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n invited' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='you to %X' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='typing' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='is typing' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='typing-stopped' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='has stopped typing' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='warned' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%W' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='has warned you' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='topic-changed' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n changed the topic' clipping='ellipsis-middle' width='0' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-13%' />
				<text format='of %C to' clipping='ellipsis-middle' width='0' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='%r' clipping='ellipsis-middle' width='0' />
			</item>
		</notification>
		<notification type='file-remote-cancel' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n canceled the' clipping='ellipsis-middle' width='0' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-13%' />
				<text format='tranfer of file' clipping='ellipsis-middle' width='0' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='"%X"' clipping='ellipsis-middle' width='0' />
			</item>
		</notification>
		<notification type='file-recv-complete' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
