			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='has signed off' clipping='ellipsis-middle' />
			</item>
		</notification>
