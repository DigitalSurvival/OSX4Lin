			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='has messaged you' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='chat-message' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
				<icon type='protocol' size='little' />
			</item>
			<item type='text'>
