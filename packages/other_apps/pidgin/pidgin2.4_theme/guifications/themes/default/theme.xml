				<position value='south-west' />
				<h_offset value='5' />
				<v_offset value='-5' />
			<icon type='protocol' size='little' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-25%' />
				<text format='%n' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<v_offset value='-12' />
				<text format='said your nick' clipping='ellipsis-middle' />
			</item>
			<item type='text'>
				<position value='center' />
				<text format='in %c' clipping='ellipsis-middle' />
			</item>
		</notification>
		<notification type='chat-join' background='background.png'>
			<item type='icon'>
				<position value='south-west' />
				<h_offset value='5' />
